#平台设置
CONFIG_TARGET_x86=y#平台
CONFIG_TARGET_x86_64=y#架构
CONFIG_TARGET_x86_64_Generic=y#型号
#固件设置
#CONFIG_EFI_IMAGES=y#编译UEFI固件
CONFIG_TARGET_IMAGES_GZIP=y#固件压缩
CONFIG_TARGET_KERNEL_PARTSIZE=30#固件内核分区大小
CONFIG_TARGET_ROOTFS_PARTSIZE=400#固件ROOTFS大小
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y#ipv6支持
CONFIG_PACKAGE_ipv6helper=y#ipv6支持
#主题
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-theme-netgear=y
#软件包
CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_screen=y
CONFIG_PACKAGE_tree=y
CONFIG_PACKAGE_wget=y
#插件
CONFIG_PACKAGE_luci-app-webadmin=y#Web管理页面设置
CONFIG_PACKAGE_luci-app-filetransfer=y#文件传输（可web安装ipk包）
CONFIG_PACKAGE_luci-app-ramfree=y#释放内存
CONFIG_PACKAGE_luci-app-nlbwmon=y#网络带宽监视器
CONFIG_PACKAGE_luci-app-advancedsetting=y#系统高级设置（Li大内插件）
CONFIG_PACKAGE_luci-app-attendedsysupgrade=y#固件更新升级相关
CONFIG_PACKAGE_luci-app-autoreboot=y#支持计划重启
CONFIG_PACKAGE_luci-app-uugamebooster=y#UU网游加速器
CONFIG_PACKAGE_luci-app-sfe=y#Turbo ACC网络加速
CONFIG_PACKAGE_luci-app-zerotier#ZeroTier内网穿透
#添加第三方插件
CONFIG_PACKAGE_luci-app-vssr=y#vssr
CONFIG_PACKAGE_luci-app-jd-dailybonus=y#京东签到
CONFIG_PACKAGE_luci-app-unblockneteasemusic=y#解锁网易云音乐
CONFIG_PACKAGE_luci-app-adguardhome=y#AdGuard home广告过滤
